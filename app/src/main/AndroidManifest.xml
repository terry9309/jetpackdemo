<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpackdemo">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".workmanager.WMActivity"
            android:exported="false" />
        <activity android:name=".viewmodel.ViewModelActivity" />
        <activity android:name=".dataBinding.DataBindUseActivity" />
        <activity android:name=".livedata.use3.LiveDataNextActivity" />
        <activity android:name=".livedata.use3.LiveDataUse3Activity" />
        <activity android:name=".livedata.use2.LiveDataUse2Activity" />
        <activity android:name=".livedata.use1.LiveDataActivity" />
        <activity android:name=".hilt.HiltActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".livedata.use2.MyService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>